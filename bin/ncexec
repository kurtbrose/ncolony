#!/usr/bin/env python
import os
import sys

venvHome = os.path.dirname(sys.executable)
home = os.path.normpath(os.path.join(venvHome, '../..'))
sys.path.append(home)
for line in file(os.path.join(home, "support/paths")):
    sys.path.append(line.strip())
del sys.argv[0]
d=dict(__name__='__main__')
execfile(sys.argv[0], d)
